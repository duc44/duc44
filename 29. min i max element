niz=[9,2,5,5,6,7,8,9,12,12]
console.log(niz)
for(i=0;i<10;i++){
    br=0
    brr=0
    for(ii=0;ii<10;ii++){
        if(niz[i]>=niz[ii]){
            br++
        }
        if(niz[i]<=niz[ii]){
            brr++
        }
        if(br===10){
            max=niz[i]
        }
        if(brr===10){
            min=niz[i]
        }
    }
}
console.log("Najveci je" ,max,", a najmanji",min)
